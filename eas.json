      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "distribution": "internal",
      "credentialsSource": "local",
      "env": {
        "NODE_ENV": "development",
        "METRO_MINIFY": "false",
        "EXPO_BUNDLE_MINIFY": "false",
        "BABEL_ENV": "development",
        "RN_BUNDLE_MINIFY": "false",
        "TERSER_DISABLE": "true",
        "UGLIFY_DISABLE": "true"
      }
    },
    "debug": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "distribution": "internal",
      "credentialsSource": "local",
      "env": {
        "NODE_ENV": "development",
        "METRO_MINIFY": "false",
        "EXPO_BUNDLE_MINIFY": "false",
        "BABEL_ENV": "development",
        "RN_BUNDLE_MINIFY": "false",
        "TERSER_DISABLE": "true",
        "UGLIFY_DISABLE": "true"
      }
    },
    "production": {}
  },
  "submit": {
    "production": {}
  }
